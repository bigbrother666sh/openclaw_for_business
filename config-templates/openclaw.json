{
  "auth": {
    "profiles": {
      "siliconflow:default": {
        "provider": "openai",
        "mode": "api_key"
      }
    }
  },
  "models": {
    "providers": {
      "siliconflow": {
        "api": "openai-completions",
        "baseUrl": "https://api.siliconflow.cn/v1",
        "apiKey": "",
        "models": [
          {
            "id": "moonshotai/Kimi-K2-Thinking",
            "name": "Kimi K2 Thinking",
            "input": ["text"],
            "contextWindow": 256000,
            "maxTokens": 8192
          },
          {
            "id": "zai-org/GLM-4.6V",
            "name": "GLM-4.6V",
            "input": ["text", "image"],
            "contextWindow": 128000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "siliconflow/moonshotai/Kimi-K2-Thinking"
      },
      "imageModel": {
        "primary": "siliconflow/zai-org/GLM-4.6V"
      },
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true,
    "ownerDisplay": "raw"
  },
  "hooks": {
    "internal": {
      "enabled": true,
      "entries": {
        "boot-md": {
          "enabled": true
        },
        "command-logger": {
          "enabled": true
        },
        "session-memory": {
          "enabled": true
        }
      }
    }
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "openclaw",
    "headless": false,
    "ssrfPolicy": {
      "dangerouslyAllowPrivateNetwork": true
    }
  },
  "skills": {
    "entries": {
      "browser-guide": {
        "enabled": true
      },
      "apple-notes": { "enabled": false },
      "apple-reminders": { "enabled": false },
      "bear-notes": { "enabled": false },
      "things-mac": { "enabled": false },
      "spotify-player": { "enabled": false },
      "blucli": { "enabled": false },
      "sonoscli": { "enabled": false },
      "openhue": { "enabled": false },
      "eightctl": { "enabled": false },
      "peekaboo": { "enabled": false },
      "blogwatcher": { "enabled": false },
      "summarize": { "enabled": false },
      "ordercli": { "enabled": false }
    }
  },
  "channels": {
    "feishu": {
      "enabled": true,
      "appId": "",
      "appSecret": "",
      "domain": "feishu",
      "groupPolicy": "disabled",
      "accounts": {}
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": ""
    },
    "tailscale": {
      "mode": "off",
      "resetOnExit": false
    },
    "nodes": {
      "denyCommands": [
        "camera.snap",
        "camera.clip",
        "screen.record",
        "calendar.add",
        "contacts.add",
        "reminders.add"
      ]
    }
  },
  "plugins": {
    "load": {
      "paths": ["openclaw/extensions/feishu"]
    },
    "entries": {
      "feishu": {
        "enabled": true
      }
    }
  }
}
